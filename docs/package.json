    "clsx": "^2.0.0",
    "concurrently": "^9.2.0",
    "docusaurus-plugin-openapi-docs": "^4.3.7",
    "docusaurus-theme-openapi-docs": "^4.3.7",
    "nodemon": "^3.1.10",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
